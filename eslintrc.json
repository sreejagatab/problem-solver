{
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:security/recommended"
    ],
    "plugins": ["security"],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "security/detect-non-literal-fs-filename": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    }
  }